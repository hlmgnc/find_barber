    {
        "extends": [
          "airbnb",
          "prettier",
          "prettier/@typescript-eslint",
          "prettier/babel",
          "prettier/flowtype",
          "prettier/react",
          "prettier/standard",
          "prettier/unicorn",
          "prettier/vue"
        ],
        "rules": {
          "eqeqeq": "off",
          "func-names": "off",
          "class-methods-use-this": "off"
        },
        "settings": {
          "react": {
            "version": "latest"
          }
        }
      }
